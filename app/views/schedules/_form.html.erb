<%= form_for(@schedule, :url => @url) do |f| %>
   <% if @schedule.errors.any? %>
      <div>
         <% @schedule.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
         <% end %>
      </div>
   <% end %>

   <div>
      <%= @project.name %>
   </div>
   <div>
      <%= f.collection_select(:developer_id, @developers, :id, :name) %>
   </div>
   <div>
      <%= f.label :start_date %>
      <%= f.date_select("start_date") %>
   </div>
   <div>
      <%= f.label :end_date %>
      <%= f.date_select("end_date") %>
   </div>
   <%= f.submit %>
<% end %>